#!/bin/bash

cd $(dirname $0)/..

find * -maxdepth 0 -type d \
    | grep -v '^\.'                 \
    | grep -v '^Godeps'             \
    | grep -v '^vendor'             \
    | grep -v '^bin'                \
    | grep -v '^gobin'              \
    | grep -v '^out'                \
    | grep -v '^tmp'                \
    | xargs $@
